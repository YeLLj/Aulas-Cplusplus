#include <iostream>
using namespace std;

int main () 
{   
    int tamanho = 4, num_onibus = 0, contador = 0;
    string onibus[tamanho], nome;

        for (int i = 0; i < tamanho ; i++)
        {
         onibus[i] = "disponivel";
        }

        cout << "Digite o seu nome e o número do ônibus: ";
        for (int i = 0; i < tamanho ; i++)
        {
            cin >> nome >> num_onibus;
            
            if (onibus[num_onibus] == "disponivel"){
                onibus[num_onibus] = nome;}
            else contador +=1;
                
        }
        
        for (int i = 0; i < tamanho ; i++)
        {
            cout << onibus[i] << " ";
        }

        cout << contador;
}
